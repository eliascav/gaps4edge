{
	"manifest_version": 2,
	"name": "GWorkspace launcher",
	"description": "Лаунчер приложений Google",
	"version": "1.0.0",
	"icons": {"128": "GApps.png"},
	"browser_action": {
		"default_icon": "GApps.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"https://*/*",
    	"http://*/*",
    	"tabs"]
}